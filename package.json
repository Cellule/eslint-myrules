{
  "name": "cellule-eslint-myrules",
  "version": "3.1.0",
  "description": "Define my favorite rules for eslint",
  "author": "Michael Ferris",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Cellule/eslint-myrules"
  },
  "bin": {
    "cellule-eslint-myrules": "./cliWrapper",
    "eslint": "node_modules/eslint/bin/eslint.js"
  },
  "main": "index.js",
  "scripts": {
    "lint": "node cliWrapper lint ."
  },
  "eslint-myrules": {
    "modifiers": {
      "es2015": false,
      "react": false,
      "jshost": false,
      "no-code-style": false
    }
  },
  "dependencies": {
    "babel-eslint": "^4.0.5",
    "eslint": "^1.4.3",
    "eslint-myrules-manager": "^0.4.0",
    "eslint-plugin-babel": "^2.0.0",
    "eslint-plugin-react": "^3.4.0",
    "eslint-plugin-shadow-exception": "^1.1.1"
  }
}
